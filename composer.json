{
	"name": "bunkuris/has-cache",
	"description": "HasCache trait for Laravel models with cache key managers",
	"keywords": [
		"cache"
	],
	"type": "library",
	"authors": [
		{
			"name": "DƒÅvis Puciriuss",
			"email": "d.puciriuss@gmail.com"
		}
	],
	"require": {
		"php": "^8.1",
		"ext-json": "*",
		"chillerlan/php-qrcode": "^5.0",
		"illuminate/auth": "^10.0|^11.0|^12.0",
		"illuminate/console": "^10.0|^11.0|^12.0",
		"illuminate/contracts": "^10.0|^11.0|^12.0",
		"illuminate/database": "^10.0|^11.0|^12.0",
		"illuminate/events": "^10.0|^11.0|^12.0",
		"illuminate/http": "^10.0|^11.0|^12.0",
		"illuminate/pagination": "^10.0|^11.0|^12.0",
		"illuminate/routing": "^10.0|^11.0|^12.0",
		"illuminate/support": "^10.0|^11.0|^12.0",
		"illuminate/validation": "^10.0|^11.0|^12.0"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.0",
		"larastan/larastan": "^2.4|^3.0",
		"orchestra/testbench": "^8.0|^9.0|^10.0",
		"phpunit/phpunit": "^9.0|^10.0|^11.5"
	},
	"autoload": {
		"psr-4": {
			"Bunkuris\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Bunkuris\\Tests\\": "tests/"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"Bunkuris\\HasCacheServiceProvider"
			],
			"aliases": {
				"AsyncCache": "Bunkuris\\Facades\\AsyncCache"
			}
		}
	},
	"scripts": {
        "fix-style": "php-cs-fixer fix",
		"lint": "php-cs-fixer fix --diff --dry-run",
        "phpstan": "phpstan analyse",
        "test": "phpunit",
        "test-coverage": "php ./vendor/bin/phpunit --coverage-text",
        "test-coverage-html": "php ./vendor/bin/phpunit --coverage-html phpunit-coverage"
	},
    "minimum-stability": "dev",
    "prefer-stable": true
}
